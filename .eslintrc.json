{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb",
    "airbnb/hooks",
    "plugin:prettier/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "max-len": 1,
    "no-unused-vars": 1,
    "no-trailing-spaces": 1,
    "comma-dangle": 0,
    "spaced-comment": 1,
    "object-shorthand": 1,
    "prefer-arrow-callback": 1,
    "import/prefer-default-export": 1,
    "prefer-const": 1
  }
}
